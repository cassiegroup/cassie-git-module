<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <PackageId>cassie-git-module</PackageId>
    <Version>0.8.3.2</Version>
    <Authors>cassie</Authors>
    <Company>cassie group</Company>
    <PackageTags>csharp;dotnet core;git;sdk;cassie;dotnet</PackageTags>
    <ProjectUrl>https://github.com/cassiegroup/cassie-git-module</ProjectUrl>
    <RepositoryUrl>https://github.com/cassiegroup/cassie-git-module</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <Description>
      This git library follow .netstandard 2.0. you can use it in .NET Framework and .NET core.
    </Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.FileSystem.AccessControl" Version="4.7.0" />
  </ItemGroup>

</Project>
